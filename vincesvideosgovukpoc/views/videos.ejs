<!DOCTYPE html>
<html lang="en">
    <head>
      <%- include('./partials/head'); %>
    </head>
    <body>
        <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
        <%- include('./partials/header'); %>
        <table class="govuk-table">
            <caption class="govuk-table__caption govuk-table__caption--m">Videos</caption>
            <thead class="govuk-table__head">
                <tr class="govuk-table__row">
                    <th scope="col" class="govuk-table__header">Title</th>
                    <th scope="col" class="govuk-table__header">Stock</th>
                </tr>
            </thead>
            <tbody class="govuk-table__body">
                <% videos.forEach(function(video){ %>
                    <tr class="govuk-table__row">
                        <td class="govuk-table__cell"><%= video.title %></td>
                        <td class="govuk-table__cell"><%= video.stockCount %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
          <script src="/scripts/govuk.js"></script>
          <script>
            window.GOVUKFrontend.initAll()
          </script>
    </body>
</html>